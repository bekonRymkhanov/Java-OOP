package lab2;

import java.util.Vector;

public class test {

	public static void main(String[] args) {
		Vector vec =new Vector();
		
		Circle c=new Circle("red",5);
		vec.add(c);
		Circle d=new Circle("wr",10);
		vec.add(d);

		Circle r=new Circle("red",5);
		vec.add(r);
		Circle a=new Circle("wr",10);
		vec.add(a);
		Circle k=new Circle("red",5);
		vec.add(k);
		Circle e=new Circle("wr",10);
		vec.add(e);
		for(int i=0;i<6;i++) {
			((Circle) vec.elementAt(i)).draw();
		}
	}

}
