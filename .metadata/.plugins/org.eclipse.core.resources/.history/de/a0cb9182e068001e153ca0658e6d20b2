package lab2;

public abstract class Shapes {
	char a;
	protected color col;
	protected int position;
	
	public Shapes() {
		this.a = '*';
		this.position=0;
		this.col=color.white;
	}
	
	public Shapes(char a) {
		this.position=0;
		this.col=color.white;
		this.a = a;
	}
	
    abstract void draw();
	public char getA() {
		return this.a;
	}
	
	public void setA(char a) {
		this.a = a;
	}
	
	public String toString() {
		return "\nTHIS IS ";
	}
	
}

